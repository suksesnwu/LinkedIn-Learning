   "source": [
    "def find_min(xs):\n",
    "    min_index = 0\n",
    "    for i in range(len(xs)):\n",
    "        if xs[i] < xs[min_index]:\n",
    "            min_index = i\n",
    "    return xs[min_index]\n",
    "\n",
    "xs = [3, 2, 1.5, 5, 4]\n",
    "min_value = find_min(xs)\n",
    "print(f\"The minimum value is {min_value}.\")"
   ]
